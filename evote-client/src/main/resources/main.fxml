<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<TabPane xmlns:fx="http://javafx.com/fxml" minWidth="200" maxWidth="500" minHeight="400" maxHeight="500">
    <tabs>
        <Tab text="Voter" fx:id="voterTab">
            <content>
                <fx:include fx:id="voterTabPage" source="/voterRegistration.fxml"/>
            </content>
        </Tab>
        <Tab text="Party" fx:id="partyTab">
            <content>
                <fx:include fx:id="partyTabPage" source="/party.fxml"/>
            </content>
        </Tab>
    </tabs>
</TabPane>